{
  "name": "Stripe test cards",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension with stripe credit card details",
  "homepage_url": "http://github.com/chmelevskij/stripe-cards",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Stripe Test Cards",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}